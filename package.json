{
  "name": "skydropx",
  "version": "1.0.0",
  "description": "SkydropX Developer Challenges",
  "main": "index.local.js",
  "license": "MIT",
  "scripts": {
    "check:pre-commit": "npm run lint:fix",
    "check:pre-push": "npm run build && npm run test",
    "start": "npm run build && node ./build/index.local.js",
    "dev": "nodemon --config nodemon.json src/index.local.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/index.local.ts",
    "build": "node_modules/.bin/tsc",
    "build:clean": "rm -rf dist/ && npm run build",
    "build:watch": "npm run build -- -w",
    "prepare": "husky install",
    "seed:all": "npm run build && sequelize-cli db:seed:all",
    "seed:undo": "npm run build && sequelize-cli db:seed:undo",
    "migrate": "npm run build && sequelize-cli db:migrate",
    "migrate:undo": "npm run build && sequelize-cli db:migrate:undo"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "0.13.2",
    "class-validator-jsonschema": "^3.1.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^12.0.3",
    "express": "^4.17.2",
    "husky": "^7.0.4",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "0.9.0",
    "routing-controllers-openapi": "3.1.0",
    "sequelize": "^5.22.5",
    "sequelize-typescript": "^1.1.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.1",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "~4.17.28",
    "@types/node": "^17.0.8",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.1",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.2.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}