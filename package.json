{
  "name": "skydropx",
  "version": "1.0.0",
  "description": "SkydropX Developer Challenges",
  "main": "index.local.js",
  "license": "MIT",
  "engines": {
    "node": ">=14.18"
  },
  "scripts": {
    "check:pre-commit": "npm run lint:fix",
    "check:pre-push": "npm run build && npm run test",
    "start": "node build/index.local.js",
    "dev": "nodemon --config nodemon.json src/index.local.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk src/index.local.ts",
    "build": "node_modules/.bin/tsc",
    "build:clean": "rm -rf dist/ && npm run build",
    "build:watch": "npm run build -- -w",
    "test": "jest --detectOpenHandles --config=jest.config.js",
    "prepare": "husky install",
    "seed:all": "npm run build && sequelize-cli db:seed:all",
    "seed:undo": "npm run build && sequelize-cli db:seed:undo",
    "migrate": "npm run build && sequelize-cli db:migrate",
    "migrate:undo": "npm run build && sequelize-cli db:migrate:undo",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "0.13.2",
    "class-validator-jsonschema": "^3.1.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^12.0.3",
    "express": "^4.17.2",
    "husky": "^7.0.4",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pg-native": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "0.9.0",
    "routing-controllers-openapi": "3.1.0",
    "sequelize": "^5.22.5",
    "sequelize-typescript": "^1.1.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.1",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/csvtojson": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "~4.17.28",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.8",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.2.0",
    "ts-jest": "^26.5.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
